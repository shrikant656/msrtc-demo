/* 
 * Pager Settings
 */
 .egov-pager {
  padding-left: 0;
  margin: 5px 0;
  text-align: left;
  list-style: none;
}
.egov-pager li {
  display: inline;
}
.egov-pager li > a,
.egov-pager li > span {
  display: inline-block;
  padding: 5px 14px;
  background-color: #fff;
  border: 1px solid #ddd;
  border-radius: 15px;
}
.egov-pager li > a:hover,
.egov-pager li > a:focus {
  text-decoration: none;
  background-color: #eee;
}
.egov-pager .next > a,
.egov-pager .next > span {
  float: right;
}
.egov-pager .previous > a,
.egov-pager .previous > span {
  float: left;
}
.egov-pager .disabled > a,
.egov-pager .disabled > a:hover,
.egov-pager .disabled > a:focus,
.egov-pager .disabled > span {
  color: #777;
  cursor: not-allowed;
  background-color: #fff;
}
.egov-pager:before,
.egov-pager:after{
  display: table;
  content: " ";
}
.panel-body:after{
  clear: both;
}
 
/*
 * Table column value Settings for ellipsis
 */
.egov-myWidth { min-width: 200px; max-width: 200px; }
.egov-concat { overflow: hidden; -ms-text-overflow: ellipsis; -o-text-overflow: ellipsis; text-overflow: ellipsis; white-space: nowrap; width: inherit; }

