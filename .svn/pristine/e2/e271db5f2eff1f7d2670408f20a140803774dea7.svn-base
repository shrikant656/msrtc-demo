.cprogress{
    width: 100px;
    height: 100px;
    /*line-height: 50px;*/
    background: none;
    margin: 0 auto;
    box-shadow: none;
    position: relative;
}
.cprogress:after{
    content: "";
    width: 100%;
    height: 100%;
    border-radius: 50%;
    border: 0 ;
    position: absolute;
    top: 0;
    left: 0;
}
.cprogress > span{
    width: 50%;
    height: 100%;
    overflow: hidden;
    position: absolute;
    top: 0;
    z-index: 1;
}
.cprogress .progress-left{
    left: 0;
}
.cprogress .cprogress-bar{
    width: 100%;
    height: 100%;
    background: none;
    border-width: 5px;
    border-style: solid;
    position: absolute;
    top: 0;
}
.cprogress .progress-left .cprogress-bar{
    left: 100%;
    border-top-right-radius: 80px;
    border-bottom-right-radius: 80px;
    border-left: 0;
    -webkit-transform-origin: center left;
    transform-origin: center left;
}
.cprogress .progress-right{
    right: 0;
}
.cprogress .progress-right .cprogress-bar{
    left: -100%;
    border-top-left-radius: 80px;
    border-bottom-left-radius: 80px;
    border-right: 0;
    -webkit-transform-origin: center right;
    transform-origin: center right;
    animation: loading-1 1.8s linear forwards;
}
.cprogress .progress-right .cprogress-bar.red{
    animation: loading-3 1.8s linear forwards;
}
.cprogress .progress-value{
    width: 100%;
    height: 100%;
    border-radius: 50%;
    border: 5px solid #ebebeb;
    font-size: 25px;
    line-height: 26px;
    text-align: center;
    position: absolute;
    top: 0;
    left: 0; padding-top:25px;
}
.cprogress .progress-value span{ clear:both; font-size:12px; display:block; line-height: 15px; color:#16364e;}


.cprogress.blue .cprogress-bar{
    border-color: #0F85CE;
}
.cprogress.blue .progress-value{
    color: #0F85CE;
}
.cprogress.blue .progress-left .cprogress-bar{
    animation: loading-2 1.5s linear forwards 1.8s;
}

.cprogress.red .cprogress-bar{
    border-color: #CD000A;
}
.cprogress.red .progress-value{
    color: #CD000A;
}
.cprogress.red .progress-left .cprogress-bar{
   
}


.over_hide{ overflow:hidden;}
.cont-block{height: 170px; overflow: hidden; width:100%; }
.cont-block img{ float:left;}

.form-group {
    display: block;
    overflow: hidden;
}

@keyframes loading-1{
    0%{
        -webkit-transform: rotate(0deg);
        transform: rotate(0deg);
    }
    100%{
        -webkit-transform: rotate(180deg);
        transform: rotate(180deg);
    }
}
@keyframes loading-2{
    0%{
        -webkit-transform: rotate(0deg);
        transform: rotate(0deg);
    }
    100%{
        -webkit-transform: rotate(144deg);
        transform: rotate(144deg);
    }
}
@keyframes loading-3{
    0%{
        -webkit-transform: rotate(0deg);
        transform: rotate(0deg);
    }
    100%{
        -webkit-transform: rotate(125deg);
        transform: rotate(125deg);
    }
}
@keyframes loading-4{
    0%{
        -webkit-transform: rotate(0deg);
        transform: rotate(0deg);
    }
    100%{
        -webkit-transform: rotate(36deg);
        transform: rotate(36deg);
    }
}
@keyframes loading-5{
    0%{
        -webkit-transform: rotate(0deg);
        transform: rotate(0deg);
    }
    100%{
        -webkit-transform: rotate(35deg);
        transform: rotate(35deg);
    }
}

