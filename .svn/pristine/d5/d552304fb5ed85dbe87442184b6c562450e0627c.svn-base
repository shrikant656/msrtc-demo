.header {z-index: 12;    box-shadow: 0 2px 5px rgba(0, 0, 0, 0.3);position: relative;}
.top_bar {padding:6px 0 5px;}
.clone .top_bar{border-bottom: none;}
#top_nav, #top_nav_mobi { margin:0; padding:0; }
#top_nav li, #top_nav_mobi li { float:left; list-style:none; border-left:1px solid #7fb133; border-right:1px solid #4c7e00; font-size:13px; }
#top_nav li:first-child, #top_nav_mobi li:first-child { border-left:0; }
#top_nav li:last-child, #top_nav_mobi li:last-child { border-right:0; }
#top_nav li a, #top_nav_mobi li a { display:block; float:left; padding:4px 10px; color:#FFFFFF; text-decoration:none; text-shadow:1px 1px 0 #447100; }
#top_nav li a:hover, #top_nav li a.active { color: #faf037; }
.txt_call { display:block; float:left; padding:4px 10px 4px 0; color:#ffffff; }
header.clone{position:fixed; top:-113px; left: 0; right: 0; z-index:999; transition:0.4s top cubic-bezier(.3,.73,.3,.74);}
