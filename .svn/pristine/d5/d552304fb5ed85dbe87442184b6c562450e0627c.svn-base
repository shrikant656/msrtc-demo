.loginbox1 {
    background: #ffffff;
    display: block;
    overflow: hidden;
    padding: 23px 35px 35px 35px;
    border-radius: 10px;
}
.login_card .input-container {
    position: relative;
    margin: 0 10px 25px;
}
.login_card .input-container input {
    outline: none;
    z-index: 1;
    position: relative;
    background: none;
    width: 100%;
    height: 54px;
    border: 0;
    color: #212121;
    font-size: 20px;
    font-weight: 400;
}
.login_card .input-container label {
    position: absolute;
    top: 0;
    left: 0;
    color: #757575;
    font-size: 18px;
    font-weight: 300;
    line-height: 60px;
    -webkit-transition: 0.2s ease;
    transition: 0.2s ease;
}
.login_card .input-container .bar {
    position: absolute;
    left: 0;
    bottom: 0;
    background: #757575;
    width: 100%;
    height: 1px;
}
.login_card .input-container .bar:before {
    left: 50%;
}
.login_card .input-container .bar:after {
    right: 50%;
}
.login_card .input-container .bar:before, .login_card .input-container .bar:after {
    content: '';
    position: absolute;
    background: #007efe;
    width: 0;
    height: 2px;
    -webkit-transition: .2s ease;
    transition: .2s ease;
}
.fgt_txt {
    color: #33b570 !important;
    font-weight: 400;
}
.loginbt {
    background: #ffd700 !important;
    margin: 0 10px;
}

